N//*****************************************************************************
N//
N//! Debounces the EVALBOT sensor switches when called periodically.
N//!
N//! If bump sensor debouncing is used, this function should be called
N//! periodically, for example every 10 ms.  It will check the bump sensor
N//! state and save a debounced state that can be read by the application at
N//! any time.
N//!
N//! \return None.
N//
N//*****************************************************************************
Nvoid
NBumpSensorDebouncer(void)
N{
N    static unsigned char ucBumperClockA = 0;
N    static unsigned char ucBumperClockB = 0;
N    unsigned char ucData;
N    unsigned char ucDelta;
N
N    //
N    // Read the current state of the hardware bump sensors
N    //
N    ucData = ROM_GPIOPinRead(GPIO_PORTE_BASE, GPIO_PIN_0 | GPIO_PIN_1);
X    ucData = ((long (*)(unsigned long ulPort, unsigned char ucPins))((unsigned long *)(((unsigned long *)0x01000010)[4]))[11])(0x40024000, 0x00000001 | 0x00000002);
N
N    //
N    // Determine bumpers that are in a different state from the debounced state
N    //
N    ucDelta = ucData ^ g_ucDebouncedBumpers;
N
N    //
N    // Increment the debounce counter by one
N    //
N    ucBumperClockA ^= ucBumperClockB;
N    ucBumperClockB = ~ucBumperClockB;
N
N    //
N    // Reset the debounce counter for any bumper that is unchanged
N    //
N    ucBumperClockA &= ucDelta;
N    ucBumperClockB &= ucDelta;
N
N    //
N    // Determine the new debounced bumper state based on the debounce
N    // counter.
N    //
N    g_ucDebouncedBumpers &= ucBumperClockA | ucBumperClockB;
N    g_ucDebouncedBumpers |= (~(ucBumperClockA | ucBumperClockB)) & ucData;
N}
