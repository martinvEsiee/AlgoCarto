X    ulStatus = ((long (*)(unsigned long ulPort, tBoolean bMasked))((unsigned long *)(((unsigned long *)0x01000010)[4]))[9])(0x40024000, 1);
N    if (ulStatus & LEFT_IR_SENSOR_PIN)
X    if (ulStatus & 0x00000008)
